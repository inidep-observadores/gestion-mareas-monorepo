{
  "name": "sigma-mareas-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.26.2",
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "dev:backend": "pnpm --filter sigma-backend start:dev",
    "dev:frontend": "pnpm --filter gestion-mareas-0-0-1 dev",
    "dev": "pnpm --parallel dev:backend dev:frontend",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test"
  }
}
